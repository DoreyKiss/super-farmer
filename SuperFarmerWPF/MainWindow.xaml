<Window x:Class="SuperFarmerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SuperFarmerWPF"
        mc:Ignorable="d"
        Title="MainWindow" ResizeMode="CanResize" MinHeight="540" MinWidth="960">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="resources\ResourceDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top" Margin="2,2,2,2">
            <Button Content="Main Window" Width="auto"  Click="BtnClickWellcomePg" Style="{StaticResource ButtonStyle}" />
            <Button Content="Game page" Width="auto" Click="BtnClickStartGamePg" Style="{StaticResource ButtonStyle}" />
        </StackPanel>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden" Margin="0,40,0,0" BorderBrush="Black" BorderThickness="2"/>
    </Grid>
    

</Window>
